{
  "name": "stellar-js",
  "version": "0.1.0",
  "description": "A modern fullstack JavaScript framework combining React with microservices architecture",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "stellar": "./cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint . --ext .ts,.tsx --max-warnings=0",
    "lint:fix": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "prepare": "husky install",
    "prepublishOnly": "npm run test:ci && npm run lint",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "validate": "npm run type-check && npm run lint && npm run test:ci"
  },
  "keywords": [
    "react",
    "framework",
    "fullstack",
    "microservices",
    "nodejs",
    "typescript"
  ],
  "author": "Rahman Azhar",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.14.0",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "jsonwebtoken": "^9.0.0",
    "bcryptjs": "^2.4.3",
    "mongoose": "^7.3.1",
    "commander": "^11.0.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "fs-extra": "^11.1.1",
    "webpack": "^5.88.1",
    "webpack-dev-server": "^4.15.1"
  },
  "devDependencies": {
    "@types/react": "^18.2.14",
    "@types/react-dom": "^18.2.6",
    "@types/express": "^4.17.17",
    "@types/node": "^20.3.2",
    "@types/cors": "^2.8.13",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/bcryptjs": "^2.4.2",
    "@types/jest": "^29.5.2",
    "typescript": "^5.1.5",
    "ts-node-dev": "^2.0.0",
    "ts-loader": "^9.4.4",
    "jest": "^29.5.0",
    "ts-jest": "^29.1.0",
    "eslint": "^8.43.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "prettier": "^2.8.8",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "@commitlint/cli": "^17.6.6",
    "@commitlint/config-conventional": "^17.6.6",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "mini-css-extract-plugin": "^2.7.6",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "terser-webpack-plugin": "^5.3.9",
    "@testing-library/react": "^14.0.0",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "vitepress": "^1.0.0-beta.3"
  },
  "peerDependencies": {
    "react": ">=17.0.0",
    "react-dom": ">=17.0.0"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rahmanazhar/StellarJS.git"
  },
  "bugs": {
    "url": "https://github.com/rahmanazhar/StellarJS/issues"
  },
  "homepage": "https://github.com/rahmanazhar/StellarJS",
  "files": [
    "dist",
    "cli",
    "templates",
    "LICENSE",
    "README.md"
  ]
}
